{
    "name": "ebrana/elasticsearch-extension",
    "description": "Ebrana elasticsearch extension for Nette",
    "type": "extension",
    "require": {
        "php": ">= 8.2 <=8.4",
        "nette/application": "~v3.2.5",
        "nette/caching": "~v3.3.1",
        "nette/di": "~v3.2.2",
        "tracy/tracy": "~v2.10.3",
        "ebrana/elasticsearch": "dev-main",
        "netom/lz-string-php": "~1.3",
        "psr/cache": "~3.0",
        "psr/simple-cache": "~3.0"
    },
    "require-dev": {
        "phpstan/phpstan": "~1.8.9",
        "phpstan/extension-installer": "~1.1.0",
        "phpstan/phpstan-nette": "~1.1.0"
    },
    "autoload": {
        "psr-4": {
            "Ebrana\\ElasticsearchExtension\\": "src/ElasticsearchExtension/"
        }
    },
    "license": "MIT",
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
